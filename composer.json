{
    "name": "luchavez/boilerplate-generator",
    "description": "A boilerplate generator for building packages and implementing DDD for Laravel.",
    "license": "MIT",
    "authors": [
        {
            "name": "James Carlo Luchavez",
            "email": "jamescarloluchavez@gmail.com",
            "homepage": "https://www.linkedin.com/in/jsluchavez/"
        }
    ],
    "homepage": "https://github.com/luchmewep/boilerplate-generator",
    "keywords": ["Laravel", "BoilerplateGenerator"],
    "require": {
        "php": "^8.0",
        "illuminate/support": "~8|~9",
        "jeroen-g/laravel-packager": "^2.8",
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-laravel": "^1.2",
        "milroyfraser/pest-plugin-gwt": "^0.0.3",
        "pestphp/pest-plugin-faker": "^1.0",
        "knuckleswtf/scribe": "^4.1",
        "laravel/pint": "^1.2",
        "luchavez/starter-kit": "^1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~6|~7"
    },
    "suggest": {
        "luchavez/starter-kit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Luchavez\\BoilerplateGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Luchavez\\BoilerplateGenerator\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Luchavez\\BoilerplateGenerator\\Providers\\BoilerplateGeneratorServiceProvider"
            ],
            "aliases": {
                "BoilerplateGenerator": "Luchavez\\BoilerplateGenerator\\Facades\\BoilerplateGenerator"
            }
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-develop": "1.0.x-dev",
            "dev-feature": "1.0.x-dev"
        }
    }
}
